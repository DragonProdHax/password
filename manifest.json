{
    "manifest_version": 2,
    "name": "Password Protected Redirect",
    "description": "Chrome extension to prompt for a password and redirect on correct input.",
    "version": "1.0",
    "permissions": [
      "activeTab"
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png", 
      "128": "images/icon128.png"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
  }
  